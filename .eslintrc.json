{
    "extends": ["airbnb", "prettier"],
    "env": {
        "commonjs": true,
        "es6": true,
        "node":true,
        "jest": true
    },
    "plugins": ["prettier", "jest"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "error",
        "global-require": "off",
        "func-names": "off",
        "no-param-reassign": "off",
        "no-console": "error"
    }
}
